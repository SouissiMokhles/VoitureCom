{% extends 'base.html.twig' %}

{% block title %}Models{% endblock %}

{% block body %}
    <h1>Models</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ model.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ model.nom }}</td>
            </tr>
            <tr>
                <th>Annee</th>
                <td>{{ model.annee }}</td>
            </tr>
            <tr>
                <th>Couleur</th>
                <td>{{ model.couleur }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_models_index') }}">back to list</a>

    <a href="{{ path('app_models_edit', {'id': model.id}) }}">edit</a>

    {{ include('models/_delete_form.html.twig') }}
{% endblock %}
